{
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "keyFile": "../../Autofac.snk",
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "Autofac": "4.0.0-rc3-*",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "type": "build",
      "version": "1.1.0"
    },
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "System.Runtime.Analyzers": {
      "type": "build",
      "version": "1.1.0"
    }
  },
  "description": "Autofac extension for multitenant application support.",
  "frameworks": {
    "net451": {},
    "netstandard1.1": {}
  },
  "packOptions": {
    "iconUrl": "https://cloud.githubusercontent.com/assets/1156571/13684110/16b8f152-e6bf-11e5-84ae-22c66c6d351a.png",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "owners": [
      "Autofac Contributors"
    ],
    "projectUrl": "http://autofac.org",
    "repository": {
      "type": "git",
      "url": "https://github.com/autofac/Autofac.Multitenant"
    },
    "requireLicenseAcceptance": false,
    "summary": "Multitenant applications allow individual tenants to override dependencies rather than having a simple root container for everyone. This extension provides support for an application container and tenant-specific overrides using Autofac.",
    "tags": [
      "autofac",
      "di",
      "ioc",
      "dependencyinjection"
    ]
  },
  "version": "4.0.0-rc3-*"
}